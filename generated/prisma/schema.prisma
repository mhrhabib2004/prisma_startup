generator client {
  provider = "prisma-client-js"
  output   = "../generated/prisma"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model post {
  id         Int      @id @default(autoincrement())
  titel      String
  content    String
  published  Boolean  @default(false)
  authorName String?
  createAt   DateTime @default(now())
  updateAt   DateTime @updatedAt
}
